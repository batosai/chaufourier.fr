{
  "typescript": true,
  "commands": [
    "./commands",
    "@adonisjs/core/build/commands/index.js",
    "@adonisjs/repl/build/commands",
    "@adonisjs/bouncer/build/commands",
    "@adonisjs/lucid/build/commands",
    "@adonisjs/mail/build/commands",
    "adonis-lucid-filter/build/commands",
    "@jrmc/attachment-advanced/build/commands",
    "@melchyore/adonis-cache/build/commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    "./start/bouncer",
    "./start/events",
    "./start/mail",
    "./start/validator",
    "./start/view",
    "./start/globals"
  ],
  "providers": [
    "./providers/AppProvider",
    "./providers/Up/Provider",
    "@adonisjs/core",
    "@adonisjs/session",
    "@adonisjs/view",
    "@adonisjs/shield",
    "@adonisjs/bouncer",
    "@adonisjs/lucid",
    "@adonisjs/i18n",
    "@adonisjs/mail",
    "@adonisjs/lucid-slugify",
    "@adonisjs/auth",
    "adonis-lucid-filter",
    "@jrmc/attachment-advanced",
    "@melchyore/adonis-cache"
  ],
  "metaFiles": [
    {
      "pattern": "public/**",
      "reloadServer": false
    },
    {
      "pattern": "resources/views/**/*.edge",
      "reloadServer": false
    },
    "resources/lang/**/*.(json|yaml)"
  ],
  "aceProviders": ["@adonisjs/repl"],
  "tests": {
    "suites": [
      {
        "name": "functional",
        "files": ["tests/functional/**/*.spec(.ts|.js)"],
        "timeout": 60000
      },
      {
        "name": "browser",
        "files": ["tests/browser/**/*.spec.ts"],
        "timeout": 30000
      }
    ]
  },
  "testProviders": ["@japa/preset-adonis/TestsProvider"]
}
