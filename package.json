{
  "name": "chaufourier.fr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "node ace test",
    "test:dev": "node ace test --watch",
    "test:functional:dev": "node ace test functional --watch",
    "test:browser:dev": "node ace test browser --watch",
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "dockerAttach": "docker exec -it adonis_app /bin/sh",
    "dace": "docker exec -it adonis_app node ace"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.6",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@editorjs/code": "^2.9.0",
    "@editorjs/delimiter": "^1.4.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/embed": "^2.7.0",
    "@editorjs/header": "^2.8.1",
    "@editorjs/inline-code": "^1.5.0",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/nested-list": "^1.4.2",
    "@editorjs/quote": "^2.6.0",
    "@editorjs/table": "^2.3.0",
    "@editorjs/underline": "^1.1.0",
    "@japa/browser-client": "^1.2.0",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "^4.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@uppy/core": "^3.8.0",
    "@uppy/dashboard": "^3.7.1",
    "@uppy/image-editor": "^2.4.0",
    "@uppy/xhr-upload": "^3.6.0",
    "adonis-preset-ts": "^2.1.0",
    "alpinejs": "^3.13.3",
    "autoprefixer": "^10.4.16",
    "clipboard": "^2.0.11",
    "editorjs-drag-drop": "^1.1.13",
    "editorjs-hyperlink": "^1.0.6",
    "editorjs-undo": "^2.0.26",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "highlight.js": "^11.9.0",
    "htmx.org": "^1.9.10",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.40.1",
    "postcss": "^8.4.33",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.2.1",
    "sqlite3": "^5.1.7",
    "table-driven-html-reporter": "^1.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3",
    "unpoly": "^3.7.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "youch": "^3.3.3",
    "youch-terminal": "^2.2.3"
