{
  "name": "chaufourier.fr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pretest": "node ace build --production",
    "test": "node ace test",
    "test:dev": "node ace test --watch",
    "test:functional:dev": "node ace test functional --watch",
    "test:browser:dev": "node ace test browser --watch",
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "dockerAttach": "docker exec -it adonis_app /bin/sh",
    "dace": "docker exec -it adonis_app node ace"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/warning": "^1.3.0",
    "@japa/browser-client": "^1.2.0",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "^4.3.0",
    "@tailwindcss/typography": "^0.5.9",
    "adonis-preset-ts": "^2.1.0",
    "alpinejs": "^3.12.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^10.0.0",
    "playwright": "^1.34.2",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "table-driven-html-reporter": "^1.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.9",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.1.1",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/lucid-slugify": "^2.2.1",
    "@adonisjs/mail": "^8.2.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/view": "^6.2.0",
    "@bomdi/codebox": "^2.0.0",
    "@editorjs/editorjs": "^2.27.0",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/nested-list": "^1.3.0",
    "@editorjs/table": "^2.2.1",
    "@editorjs/underline": "^1.1.0",
    "@iconify-json/akar-icons": "^1.1.17",
    "@iconify-json/fa-solid": "^1.1.4",
    "@jrmc/attachment-advanced": "^1.0.2",
    "@jrmc/edge-components": "^1.6.1",
    "adonis-lucid-filter": "^4.1.0",
    "edge-iconify": "^1.0.1",
    "editorjs-drag-drop": "^1.1.13",
    "editorjs-hyperlink": "^1.0.6",
    "editorjs-undo": "^2.0.22",
    "luxon": "^3.3.0",
    "phc-argon2": "^1.1.4",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.17.13",
    "unpoly": "^3.1.1",
    "uuid": "^9.0.0"
  },
  "volta": {
    "node": "18.16.0"
  }
}
