
<div x-data="editor">
  <div :id="id"></div>
  @jrmc.form.control({ name: 'body', label: false })
    @!jrmc.form.textarea({ 'x-ref': 'data', value: ($context.object && $context.object['body'] ? (typeof $context.object['body'] === 'string' ? $context.object['body'] : stringify($context.object['body'])) : '') })
  @end
</div>
