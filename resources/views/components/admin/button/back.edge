@if(up.isDrawer())
  @jrmc.button({ 'up-dismiss': '', class: 'btn-circle btn-primary z-30 fixed bottom-20 lg:bottom-10 left-10 mb-safe' })
    @svg('fa-solid:arrow-left')
  @end
@else
  @jrmc.button.link({
    href: $props.href,
    class: 'btn-circle btn-primary z-30 fixed bottom-20 lg:bottom-10 left-10 lg:left-96 mb-safe',
  })
    @svg('fa-solid:arrow-left')
  @end
@end
